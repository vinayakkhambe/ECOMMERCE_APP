spring:
 application:
   name: api-gateway
 cloud:
   gateway:
     routes:
       - id: user-service
         uri: lb://user-service
         predicates:
           - Path=/user/**

       - id: ecom-service
         uri: lb://ecom-service
         predicates:
           - Path=/ecom-service/**
  

server:
  port: 8088

eureka:
  client:
    service-url:
      defaultZone : http://localhost:8085/eureka/