spring:
 application:
    name: api-gateway
 cloud:
   discovery:
      enabled: true
   gateway:
     routes:
       - id: user-service
         uri: lb://user-service
         predicates:
           - Path=/auth/**
 


eureka:
  client:
    service-url:
      defaultZone : http://localhost:8085/eureka/
      
     
